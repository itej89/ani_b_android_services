<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/ImgYellowCircleWShawdow"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentDescription="ImgYellowCircleWShawdow"
        app:srcCompat="@drawable/yellowcirclewshawdow" />

    <ImageView
        android:id="@+id/ImgYellowAnchor"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentDescription="ImgYellowAnchor"
        app:srcCompat="@drawable/yellowanchor" />
</FrameLayout>
